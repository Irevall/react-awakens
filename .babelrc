{
  "presets": [
    ["@babel/preset-env", {
      "targets": {"browsers": ["> 1%", "not ie <= 11"] },
      "modules": false
    }], "@babel/preset-react"],
  "plugins": ["@babel/plugin-syntax-dynamic-import", ["@babel/plugin-proposal-decorators",
    { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }]
  ]
}